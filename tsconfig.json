{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "rootDir": "./src",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "outDir": "./dist"
    },
    "include": [
      "app.ts",
      "src/**/*.ts",
      "src/swagger/swagger.ts"
, "src/app.ts"    ],
